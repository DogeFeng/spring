<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            https://www.springframework.org/schema/context/spring-context.xsd">
    <!-- 进行扫描包的设置，如果要设置多个包，则可以使用“,”拆分 -->
    <context:component-scan base-package="com.ytinf.dao.impl,com.ytinf.service.impl"/>
    <bean id="item" class="com.ytinf.vo.Item">
        <property name="title" value="#{'可爱的小李老师'.substring(3,5)}"/>
        <property name="createtion" value="#{new java.util.Date()}"/>
        <property name="contents" value="#{{'看书','研究技术','上网'}}"/>
    </bean>
</beans>